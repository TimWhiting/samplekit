{
	"scopeName": "svelte-comment.latex",
	"fileTypes": [],
	"injectionSelector": "L:source.svelte",
	"patterns": [
		{
			"begin": "(<!--\\s*\\\\\\[)",
			"end": "(\\\\\\]\\s*-->)",
			"beginCaptures": { "1": { "name": "keyword.control" } },
			"endCaptures": { "1": { "name": "keyword.control" } },
			"contentName": "text.tex.latex",
			"patterns": [{ "include": "text.tex.latex" }]
		},
		{
			"begin": "(<!--\\s*\\\\\\()",
			"end": "(\\\\\\)\\s*-->)",
			"beginCaptures": { "1": { "name": "keyword.control" } },
			"endCaptures": { "1": { "name": "keyword.control" } },
			"contentName": "text.tex.latex",
			"patterns": [{ "include": "text.tex.latex" }]
		}
	]
}
